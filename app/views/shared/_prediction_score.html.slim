td class=((defined?(tdclass) ? tdclass : "col-xs-1") + " " + (prediction.score==1? "success" : (prediction.score==2? "danger"  : "") )) 
  =defined?(score) ? score : prediction.mainscores
  span  | 
  =defined?(reward) ? (prediction.score==2? (prediction.cup.match_fee*prediction.cup.active_users_count*0.5/prediction.match.prediction_winners_count).round : '') : ''
